{
  "name": "tradesense-svelte",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "dev": "vite dev --port 3001",
    "build": "svelte-kit sync && vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "audit": "npm audit",
    "audit:fix": "npm audit fix",
    "audit:fix:force": "npm audit fix --force",
    "audit:ci": "npm audit --audit-level=high",
    "audit:report": "node scripts/security-audit.js"
  },
  "devDependencies": {
    "@sveltejs/adapter-vercel": "^5.8.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@types/node": "^24.0.13",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.3"
  },
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@tiptap/core": "^3.0.1",
    "@tiptap/extension-highlight": "^3.0.1",
    "@tiptap/extension-placeholder": "^3.0.1",
    "@tiptap/extension-text-style": "^3.0.1",
    "@tiptap/extension-underline": "^3.0.1",
    "@tiptap/pm": "^3.0.1",
    "@tiptap/starter-kit": "^3.0.1",
    "@types/lodash-es": "^4.17.12",
    "@vercel/speed-insights": "^1.2.0",
    "axios": "^1.6.2",
    "chart.js": "^4.5.0",
    "chartjs-adapter-date-fns": "^3.0.0",
    "date-fns": "^4.1.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.1",
    "lightweight-charts": "^4.1.0",
    "lodash-es": "^4.17.21",
    "lucide-svelte": "^0.525.0",
    "marked": "^16.0.0",
    "qrcode": "^1.5.4",
    "svelte-dnd-action": "^0.9.63",
    "zustand": "^4.4.7"
  }
}
